{
  "expo": {
    "name": "OrangeMantra Services",
    "slug": "orangemantra-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#EA7C25"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.orangemantra.services"
    },
    "android": {
      "package": "com.orangemantra.services",
      "backgroundColor": "#EA7C25"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "orangemantra"
  }
}